@{
    ViewData["Title"] = "Home Page";
}

<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <title>
        Complemento de Pago
    </title>
</head>
<body>
    <div>
        <h2>Datos del Cliente</h2>
        <div class="d-inline-block">
            <label for="rfc" class="form-label">RFC</label>
            <input type="text" class="form-control w-auto" id="rfc" placeholder="RFC">
        </div>
        <div class="mb-3 d-inline-block">
            <label for="idRazonS" class="form-label">Nombre o Razon Social</label>
            <input type="text" class="form-control w-auto" id="idRazonS" placeholder="Nombre o Razon Social">
        </div>
        <div class="mb-3 d-inline-block">
            <label for="idCodigoP" class="form-label">Codigo Postal</label>
            <input type="text" class="form-control w-auto" id="idCodigoP" placeholder="Codigo Postal">
        </div>
        <div class="mb-3 d-inline-block">
            <label for="idRegimenF" class="form-label">Regimen Fiscal</label>
            <input type="text" class="form-control w-auto" id="idRegimenF" placeholder="Regimen Fiscal">
        </div>
    </div>
    <div>
        <h3>Concepto</h3>
        <div class="d-inline-block">
            <label for="idCiudad" class="form-label">Ciudad</label>
            <input type="text" class="form-control w-auto" id="idCiudad" placeholder="Ciudad">
        </div>
        <div class="mb-3 d-inline-block">
            <label for="idClaveP" class="form-label">Clave de Producto</label>
            <input type="text" class="form-control w-auto" id="idClaveP" placeholder="Clave de Producto">
        </div>
        <div class="mb-3 d-inline-block">
            <label for="idUnidad" class="form-label">Unidad</label>
            <input type="text" class="form-control w-auto" id="idUnidad" placeholder="Unidad">
        </div>
        <div class="mb-3 d-inline-block">
            <label for="idDescripcion" class="form-label">Descripcion de Unidad</label>
            <input type="text" class="form-control w-auto" id="idDescripcion" placeholder="Descripcion de Unidad">
        </div>
        <div class="mb-3 d-inline-block">
            <label for="idImporte" class="form-label">Importe</label>
            <input type="text" class="form-control w-auto" id="idImporte" placeholder="Importe">
        </div>
    </div>
    <div>
        <h4>Recepcion de Pago</h4>
        <div class="d-inline-block">
            <label for="" class="form-label">Fecha de Pago</label>
            <input type="text" class="form-control w-auto" id="" placeholder="Fecha de Pago">
        </div>
        <div class="mb-3 d-inline-block">
            <label for="idFormaP" class="form-label">Forma de Pago*</label>
            <input type="text" class="form-control w-auto" id="idFormaP*" placeholder="Forma de Pago*">
        </div>
        <div class="mb-3 d-inline-block">
            <label for="idMoneda" class="form-label">Moneda*</label>
            <input type="text" class="form-control w-auto" id="idMoneda" placeholder="Moneda*">
        </div>
        <div class="mb-3 d-inline-block">
            <label for="idTipoC" class="form-label">Tipo de Cambio</label>
            <input type="text" class="form-control w-auto" id="idTipoC" placeholder="Tipo de Cambio">
        </div>
        <div class="mb-3 d-inline-block">
            <label for="idMonto" class="form-label">Monto</label>
            <input type="text" class="form-control w-auto" id="idMonto" placeholder="Monto">
        </div>
        <div class="mb-3 d-inline-block">
            <label for="idNumeroO" class="form-label">Numero de Operacion*</label>
            <input type="text" class="form-control w-auto" id="idNumeroO" placeholder="Numero de Operacion*">
        </div>
    </div>
    <div>
        <h4>Busqueda de Factura</h4>
        <div class="d-inline-block">
            <label for="idBusquea" class="form-label">Busqueda por folio Fiscal</label>
            <input type="text" class="form-control w-auto" id="idBusquea" placeholder="Busqueda por folio Fiscal">
        </div>
    </div>
    <div class="text-center">
        <a asp-area="" asp-controller="Home" asp-action="Index" class="btn btn-primary btn-lg">Buscar</a>
    </div>

</body>

</html>
